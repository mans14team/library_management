{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "${DOCKER_USERNAME}/spring-app:${IMAGE_TAG}",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 80
    }
  ],
  "Logging": "/var/log/nginx",
  "Environment": [
    {
      "Name": "SPRING_PROFILES_ACTIVE",
      "Value": "prod"
    },
    {
      "Name": "SERVER_PORT",
      "Value": "8080"
    }
  ]
}