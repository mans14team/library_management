{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "${DOCKER_USERNAME}/spring-app:${IMAGE_TAG}",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 80
    }
  ],
  "Volumes": [],
  "Logging": "/var/log/nginx",
  "Command": [
    "java",
    "-Dserver.port=8080",
    "-Djava.security.egd=file:/dev/./urandom",
    "-XX:+UseContainerSupport",
    "-XX:MaxRAMPercentage=75.0",
    "-Dspring.profiles.active=prod",
    "-jar",
    "app.jar"
  ]
}