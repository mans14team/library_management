{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "anlee20/spring-app:${IMAGE_TAG}",
    "Update": "true"
  },
  "Authentication": {
    "Bucket": "",
    "Key": "/home/ec2-user/.docker/config.json"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 8080
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/log/eb-docker/containers/app",
      "ContainerDirectory": "/app/logs"
    }
  ],
  "Logging": "/var/log/eb-docker/containers/app",
  "Memory": 512
}