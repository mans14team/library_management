{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "${DOCKER_USERNAME}/spring-app:${IMAGE_TAG}",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 80
    }
  ],
  "Volumes": [
    {
      "HostDirectory": "/var/log/app",
      "ContainerDirectory": "/app/logs"
    }
  ],
  "Logging": "/var/log/eb-docker/containers/app",
  "Memory": 512
}